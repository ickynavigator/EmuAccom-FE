{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "plugin:react/recommended",
    "next/core-web-vitals",
    "eslint-config-next",
    "airbnb",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "import/no-named-as-default": 0,
    "react/jsx-props-no-spreading": ["off"],
    "react/prop-types": ["off"]
  }
}
